{
    "name": "dxsravel/essentials",
    "type": "library",
    "description": "DxsRavel Essentials Libs",
    "keywords": [
        "daxes",
        "dxsravel",
        "laravel"
    ],
    "homepage": "https://github.com/DxsRavel/Essentials",
    "license": "MIT",
    "authors": [
        {
            "name": "Laurence HR",
            "email": "laurencehr@gmail.com",
            "homepage": "http://daxes.net",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3"
    },
    "require-dev": {
        
    },
    "autoload": {
        "psr-4": {            
            "DxsRavel\\Essentials\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {            
            "DxsRavel\\Essentials\\": "tests"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('vendor/dxsravel/essentials/views', 'resources/views/DxsRavel');\""
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    },
    "minimum-stability": "dev"
}
